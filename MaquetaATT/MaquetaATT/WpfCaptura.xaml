<Window x:Class="MaquetaATT.WpfCaptura"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MaquetaATT"
        mc:Ignorable="d"
        Title="WpfCaptura" Height="600" Width="800">
    <DockPanel>
        <StackPanel Margin="20,20" HorizontalAlignment="Center">
            <StackPanel Orientation="Horizontal">
                <TextBlock Margin="10,5">Cliente:</TextBlock>
                <TextBox Name="txtNombreAp1" Margin="10,5" MinWidth="200" HorizontalAlignment="Stretch">Apellido paterno</TextBox>
                <TextBox Name="txtNombreAp2" Margin="10,5" MinWidth="200" HorizontalAlignment="Stretch">Apellido materno</TextBox>
                <TextBox Name="txtNombreNombres" Margin="10,5" MinWidth="200">Apellido paterno</TextBox>
            </StackPanel>            
        </StackPanel>
    </DockPanel>
</Window>
